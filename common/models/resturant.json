{
  "name": "resturant",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required":true
    },
    "part": {
      "type": "string",
      "description": "分店所在区",
      "required":true
    },
    "position": {
      "type": "GeoPoint",
      "description": "经纬度（地理位置）",
      "required":true
    },
    "businesstime": {
      "type": {
        "start":{
          "h":"number",
          "m":"number"
        },
        "end":{
          "h":"number",
          "m":"number"
        }
      },
      "description": "营业实际（开始事件，结束时间） 例子9:30，20:10",
      "required":true
    },
    "isserver": {
      "type": "Boolean",
      "description": "是否在服务状态",
      "default":false
    },
    "address": {
      "type": {
        "province":"string",
        "city":"string",
        "district":"string",
        "detail":"string"
      },
      "description": "具体地理位置",
      "required":true
    },
    "logo":{
      "type":"string",
      "description":"店铺logo",
      "required":true
    }
  },
  "validations": [],
  "relations": {
    "dusers": {
      "type": "hasMany",
      "model": "dusers",
      "foreignKey": "resid"
    },
    "queuings": {
      "type": "hasMany",
      "model": "queuing",
      "foreignKey": "resid"
    },
    "tables": {
      "type": "hasMany",
      "model": "table",
      "foreignKey": "resid"
    },
    "comments": {
      "type": "hasMany",
      "model": "comment",
      "foreignKey": "resid"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
